{
  "name": "playwright",
  "version": "1.0.0",
  "description": "e2e testing UI",
  "main": "index.js",
  "scripts": {
    "start": "npx playwright test",
    "report": "allure serve",
    "pw": "playwright test",
    "pw:debug": "PWDEBUG=1 npm run pw",
    "pw:ui": "playwright test --ui",
    "pw:view": "playwright test --headed",
    "pw:visual:update": "npm run pw -- --update-snapshots",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
